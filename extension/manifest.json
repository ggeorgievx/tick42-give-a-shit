{
  "manifest_version": 2,
  "name": "Tick42 Extension",
  "version": "1.0",
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "lib/tick42-glue-core.js",
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "assets/tick42.png"
  }
}
